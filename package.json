{
  "name": "yeoskin-dashboard",
  "version": "2.0.0",
  "description": "Yeoskin Ops Admin Dashboard - Enterprise Grade (Next.js)",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "security:check": "node scripts/check-secrets.js",
    "precommit": "npm run lint && npm run security:check && npm run test",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "setup:creator": "node scripts/setup-test-creator.js",
    "test:creator": "node scripts/test-all.js",
    "test:quick": "node scripts/check-schema.js",
    "migrate": "node scripts/run-migration.js",
    "analyze": "ANALYZE=true next build",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./reports/lighthouse.html"
  },
  "dependencies": {
    "@sparticuz/chromium": "^131.0.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.39.0",
    "@upstash/ratelimit": "^2.0.0",
    "@upstash/redis": "^1.34.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^12.27.2",
    "lucide-react": "^0.294.0",
    "next": "^15.0.0",
    "pino": "^9.0.0",
    "puppeteer-core": "^23.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^2.10.0",
    "resend": "^6.8.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.0",
    "@playwright/test": "^1.42.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.30",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lighthouse": "^13.0.1",
    "pg": "^8.17.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
